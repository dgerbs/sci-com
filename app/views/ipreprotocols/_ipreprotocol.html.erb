<h2>Immunoprecipitation protocols for <%= @antibody.antibody %>: </h2>
<table class="table table-striped">
  <thead>
    <tr>
      <th>By</th>
      <th>Images</th>
    </tr>
  </thead>

  <tbody>
   <% @ipreprotocols.each do |protocol| %>
    <tr>
      <td>
        <% if current_user == protocol.user %>
          You
        <% else %>
          <%= link_to protocol.user.email, protocol.user %>
        <% end %>
      </td>
      <td>
        <!-- Display protocol.images.count -->
        0
      </td>
      <td><%= link_to 'Show', protocol, class: "btn-link" %></td>
    </tr>
  <% end %>
  </tbody>
</table>