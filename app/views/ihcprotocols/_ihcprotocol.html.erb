<h2>Immunohistochemistry protocols:</h2>
<table class="table table-striped">
  <thead>
    <tr>
      <th>By</th>
      <th>Tissue</th>
      <th>Antibody</th>
      <th>Image</th>
    </tr>
  </thead>

  <tbody>
    <% @ihcprotocols.each do |protocol| %>
      <tr>
        <td>
          <% if current_user == protocol.user %>
            You
          <% else %>
            <%= link_to protocol.user.name, protocol.user %>
          <% end %>
        </td>
        <td>
          <%= protocol.tissue %>
        </td>
        <td>
          <%= link_to protocol.antibody.antibody, protocol.antibody %>
        </td>
        <td>
          <%= link_to 'Show', protocol, class: "btn-link" %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>