<table class="table table-striped">
  <thead>
    <tr>
      <th>Tissue</th>
      <th>Primary description</th>
      <th>Posted</th>
      <th>By</th>
    </tr>
  </thead>

  <tbody>
   <% @ihcprotocols.each do |protocol| %>
    <tr>
      <td><%= protocol.tissue %></td>
      <td><%= protocol.primary_description %></td>
      <td><%= time_ago_in_words(protocol.created_at) %> ago</td>
      <td>
        <% if current_user == protocol.user %>
          You
        <% else %>
          <%= link_to protocol.user.email, protocol.user %>
        <% end %>
      </td>
      <td><%= link_to 'Show', protocol, class: "btn-link" %></td>
    </tr>
  <% end %>
  </tbody>
</table>