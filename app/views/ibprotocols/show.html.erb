<div class="row">
  <div class="col-md-5">
    <div class="panel panel-info">
      <div class="panel-heading">
        <h3>
          <b>Immunoblot Protocol:</b>
        </h3>
      </div>
      <div class="panel-body">
        <p>
          <b>Cell/tissue type:</b>
          <%= @ibprotocol.cell_tissue %>
        </p>
        <p>
          <b>Lysis:</b>
          <%= @ibprotocol.lysis %>
        </p>
        <p>
          <b>Sonication:</b>
          <%= @ibprotocol.sonication %>
        </p>
        <p>
          <b>Sample size:</b>
          <%= @ibprotocol.sample_size %>
        </p>
        <p>
          <b>Sample loading:</b>
          <%= @ibprotocol.sample_loading %>
        </p>
        <p>
          <b>Gel:</b>
          <%= @ibprotocol.gel %>
        </p>
        <p>
          <b>Electrophoresis:</b>
          <%= @ibprotocol.electrophoresis %>
        </p>
        <p>
          <b>Membrane:</b>
          <%= @ibprotocol.membrane %>
        </p>
        <p>
          <b>Transfer:</b>
          <%= @ibprotocol.transfer %>
        </p>
        <p>
          <b>Blocking:</b>
          <%= @ibprotocol.blocking %>
        </p>
        <p>
          <b>Primary description:</b>
          <%= @ibprotocol.primary_description %>
        </p>
        <p>
          <b>Washes:</b>
          <%= @ibprotocol.first_wash %>
        </p>
        <p>
          <b>Secondary description:</b>
          <%= @ibprotocol.secondary_description %>
        </p>
        <p>
          <b>Washes:</b>
          <%= @ibprotocol.second_wash %>
        </p>
        <p>
          <b>Detection:</b>
          <%= @ibprotocol.detection %>
        </p>
        <p>
          <b>Helpful information about my protocol:</b>
          <%= @ibprotocol.information %>
        </p>
        <p>
          <b>by:</b>
          <% if current_user == @ibprotocol.user %>
            You
          <% else %>
            <%= link_to @ibprotocol.user.name, @ibprotocol.user %>
          <% end %>
        </p>
        <p>
          <b>posted:</b>
          <%= time_ago_in_words(@ibprotocol.created_at) %> ago.
        </p>
        <p>
          <b>on:</b>
          <%= link_to @ibprotocol.antibody.antibody, @ibprotocol.antibody %>
        </p>
      </div>
    
      <br />
      <div class="panel-footer">
        <% if current_user == @ibprotocol.user %>
          <%= link_to 'Edit Protocol', edit_ibprotocol_path(@ibprotocol), class: "btn btn-primary" %>
          <%= link_to 'Destroy Protocol', @ibprotocol, method: :delete, data: { confirm: 'Are you sure?' }, class: "btn btn-danger" %>
        <% end %>
      </div>     
    </div>
  </div>
  <div class="col-md-7">
    <style>
    div.col-md-7 {
      border: 1px solid black;
    }
    </style>
    <p>
      Images
    </p>
  </div>
</div>